    //File
    {
        "key": "file.openFile",
        "alias": "f-o",
        "title": "File: Open a file",
        "arguments": [
            {
                "name": "fileName",
                "valueProvider": "values.systempath",
                "affinity": "FilePath"
            },
            {
                "name": "encoding",
                "valueProvider": "values.encoding",
                "optional": true
            }
        ]
    },
    {
        "key": "file.closeFile",
