[
    {
        "key": "app.commandPalette",
        "alias": "?",
        "title": "Command Palette",
        "arguments": [
            {
                "name": "commandName",
                "valueProvider": "values.commands"
            }
        ]
    },
    {
        "key": "app.toggleCommandBar",
        "alias": "cmd",
        "title": "Toggle command bar"
    },
    {
        "key": "app.toggleMessage",
        "alias": "cmd-msg",
        "title": "Display or hide error or warning message"
    },
    {
        "key": "app.changeTheme",
        "alias": "theme",
        "title": "Change visual theme",
        "arguments": [
            {
                "name": "theme",
                "valueProvider": "values.themes"
            }
        ]
    },
    {
        "key": "app.changeMode",
        "alias": "mode",
        "title": "Change language mode",
        "arguments": [
            {
                "name": "mode",
                "valueProvider": "values.modes"
            }
        ]
    },

    //Workspace
    {
        "key": "workspace.openWorkspace",
